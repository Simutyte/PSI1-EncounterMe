             xmlns:mymap="clr-namespace:Xamarin.Forms.Maps;assembly=Xamarin.Forms.Maps"
             x:Class="EncounterMe.Map">
    <ContentPage.Content>
        <StackLayout>
            <mymap:Map x:Name="mylocalmap"
               HorizontalOptions="FillAndExpand"
               VerticalOptions="FillAndExpand"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
